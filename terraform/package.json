{
  "name": "terraform",
  "version": "1.0.0",
  "main": "dist/main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "deploy": "cdktf deploy --auto-approve",
    "undeploy": "cdktf destroy  --auto-approve",
